{
  "description": "Initial version of plugin to capture academic data",
  "manifest_version": 2,
  "name": "Google Scholar Capture",
  "version": "0.5",

  "icons": {
    "48": "icons/ASU_image-48.png"
  },

  "permissions": [
    "activeTab",
    "notifications"
  ],

  "background": {
    "scripts": ["academiaCiting.js"]
  },

  "browser_action": {
    "default_title": "ASU Academia",
    "default_icon": {
      "16": "icons/ASU-32.png"
    }
  }
}
